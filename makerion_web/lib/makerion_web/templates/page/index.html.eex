<div class="container">
  <div class="card-list">
    <div class="card">
      <div class="card__body">
        <p class="card__title">Upload a New File</p>
      </div>
      <div class="button card__action">
        <%= button "", to: print_file_path(@conn, :new), method: :get, class: "plus-button plus-button--large", "data-test": "upload_new_file" %>
      </div>
    </div>
    <%= live_render(@conn, MakerionWeb.PrintFileLive.Index) %>
  </div>
</div>
